<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RandomTodayLaunch" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#4354F1</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#A3000000</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.RandomTodayLaunch.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="customChipGroupFontStyle" parent="TextAppearance.MaterialComponents.Chip">
        <item name="android:textSize">15sp</item>
        <item name="fontFamily">@font/context</item>
    </style>
`
    <style name="AppBarFont" parent="TextAppearance.MaterialComponents.Headline6">
        <item name="fontFamily">@font/context</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Theme.RandomTodayLaunch.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RandomTodayLaunch.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>